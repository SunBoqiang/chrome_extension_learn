{
  "manifest_version": 3,
  "name": "Autoreload Extensions",
  "description": "Base Level Extension, reload tags",
  "version": "1.0",
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "action": {
    "default_icon": "images/icon-128.png",
    "default_title": "Reload All Tabs"
  },
  "background": {
    "service_worker": "scripts/content.js"
  },
  "permissions": [
    "tabs",
    "alarms",
    "storage"
  ]
}